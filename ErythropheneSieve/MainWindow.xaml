<Window x:Class="ErythropheneSieveDemo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ErythropheneSieveDemo"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="label" Content="N:" FontSize="16" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbN" FontSize="16" HorizontalAlignment="Left" Height="23" Margin="32,10,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="tbResult" FontSize="16" HorizontalAlignment="Left" Height="222" Margin="32,62,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="444"/>
        <Button x:Name="btnCalc" Content="Calculate" FontSize="16" Click="BtnCalc_OnClick" HorizontalAlignment="Left" Margin="250,10,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
